function showcards() {
  document.getElementById("2").removeAttribute("disabled");
  document.getElementById("3").removeAttribute("disabled");
  document.getElementById("4").removeAttribute("disabled");
  document.getElementById("5").removeAttribute("disabled");
  document.getElementById("6").removeAttribute("disabled");
  document.getElementById("7").removeAttribute("disabled");
  document.getElementById("8").removeAttribute("disabled");
  document.getElementById("9").removeAttribute("disabled");
  document.getElementById("10").removeAttribute("disabled");
  document.getElementById("11").removeAttribute("disabled");
  document.getElementById("12").removeAttribute("disabled");
  document.getElementById("13").removeAttribute("disabled");
  document.getElementById("14").removeAttribute("disabled");
}
function hidecards() {
  document.getElementById("2").setAttribute("disabled", "1");
  document.getElementById("3").setAttribute("disabled", "1");
  document.getElementById("4").setAttribute("disabled", "1");
  document.getElementById("5").setAttribute("disabled", "1");
  document.getElementById("6").setAttribute("disabled", "1");
  document.getElementById("7").setAttribute("disabled", "1");
  document.getElementById("8").setAttribute("disabled", "1");
  document.getElementById("9").setAttribute("disabled", "1");
  document.getElementById("10").setAttribute("disabled", "1");
  document.getElementById("11").setAttribute("disabled", "1");
  document.getElementById("12").setAttribute("disabled", "1");
  document.getElementById("13").setAttribute("disabled", "1");
  document.getElementById("14").setAttribute("disabled", "1");
}
